{
  "name": "sfdx-simple-profile-retrieve",
  "displayName": "SFDX Simple Profile Retrieve",
  "description": "",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sfdx-simple-profile-retrieve.getPerms"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sfdx-simple-profile-retrieve.getPerms",
        "title": "Get Field Permissions"
      }
    ],
    "menus": {
      "explorer/context": [{
          "when": "resourceFilename =~ /field-meta/",
          "command": "sfdx-simple-profile-retrieve.getPerms",
          "group": "YourGroup@1"
    }]
  }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "del": "^6.0.0"
  },
  "languages": [
    {
      "id": "ignore",
      "filenames": [
        ".forceignore"
      ]
    },
    {
      "id": "json",
      "filenamePatterns": [
        "*.wdash",
        "*.wdf",
        "*.wdpr",
        "*.wlens",
        "*.orchestration"
      ]
    },
    {
      "id": "forcesourcemanifest",
      "aliases": [
        "ForceSourceManifest",
        "forcesourcemanifest"
      ],
      "filenamePatterns": [
        "**/manifest/**/*.xml"
      ]
    },
    {
      "id": "sfFieldMetadata",
      "aliases": [
        "sfFieldMetadata"
      ],
      "filenamePatterns": [
        "*field-meta*"
      ]
    }
  ]
}
